{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "backup_files/**/*",
    "components_disabled/**/*",
    "constants_disabled/**/*",
    "node_modules",
    ".expo",
    "web-build"
  ]
}
